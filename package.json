{
    "name": "mariana",
    "private": true,
    "version": "1.0.0",
    "type": "module",
    "description": "A version control system designed specifically for TouchDesigner projects.",
    "main": "./out/main/main.js",
    "author": "Sandrini Santiago, Brave Jeronimo, Garcia Juan Ignacio",
    "scripts": {
        "format": "prettier --write .",
        "lint": "eslint . --ext .js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix",
        "typecheck:node": "tsc --noEmit -p tsconfig.node.json --composite false",
        "typecheck:web": "tsc --noEmit -p tsconfig.web.json --composite false",
        "typecheck": "npm run typecheck:node && npm run typecheck:web",
        "start": "electron-vite preview",
        "dev": "electron-vite dev",
        "build": "npm run typecheck && electron-vite build",
        "postinstall": "electron-builder install-app-deps",
        "build:unpack": "npm run build && electron-builder --dir",
        "build:win": "npm run build && electron-builder --win",
        "build:mac": "electron-vite build && electron-builder --mac",
        "build:linux": "electron-vite build && electron-builder --linux",
        "prepare": "husky",
        "test": "jest",
        "test:watch": "jest --watch"
    },
    "dependencies": {
        "@electron-toolkit/preload": "^3.0.1",
        "@electron-toolkit/utils": "^3.0.0",
        "@fontsource/montserrat": "^5.0.18",
        "@radix-ui/react-checkbox": "^1.1.2",
        "@radix-ui/react-dialog": "^1.1.1",
        "@radix-ui/react-label": "^2.1.0",
        "@radix-ui/react-popover": "^1.1.2",
        "@radix-ui/react-slot": "^1.1.0",
        "@radix-ui/react-toast": "^1.2.2",
        "@types/hidefile": "^3.0.3",
        "@xyflow/react": "^12.1.1",
        "autoprefixer": "^10.4.20",
        "axios": "^1.7.7",
        "chokidar": "^3.6.0",
        "class-variance-authority": "^0.7.0",
        "clsx": "^2.1.1",
        "crypto": "^1.0.1",
        "electron-log": "^5.1.7",
        "electron-store": "^10.0.0",
        "electron-toolkit": "^1.0.24",
        "electron-vite": "^2.3.0",
        "framer-motion": "^11.11.17",
        "fs-extra": "^11.2.0",
        "hidefile": "^3.0.0",
        "lucide-react": "^0.427.0",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "react-icons": "^5.3.0",
        "react-router-dom": "^6.26.0",
        "simple-git": "^3.25.0",
        "tailwind-merge": "^2.5.0",
        "tailwind-scrollbar": "^3.1.0",
        "tailwindcss-animate": "^1.0.7",
        "vite-tsconfig-paths": "^4.3.2"
    },
    "devDependencies": {
        "@electron-toolkit/eslint-config-prettier": "^2.0.0",
        "@electron-toolkit/eslint-config-ts": "^2.0.0",
        "@electron-toolkit/tsconfig": "^1.0.1",
        "@types/jest": "^29.5.14",
        "@types/mocha": "^10.0.9",
        "@types/node": "^22.9.0",
        "@types/react": "^18.3.3",
        "@types/react-dom": "^18.3.0",
        "@typescript-eslint/eslint-plugin": "^7.1.1",
        "@typescript-eslint/parser": "^7.1.1",
        "@vitejs/plugin-react": "^4.3.1",
        "electron": "^31.0.2",
        "electron-builder": "^24.13.3",
        "electron-vite": "^2.3.0",
        "eslint": "^8.57.0",
        "eslint-plugin-react": "^7.34.3",
        "eslint-plugin-react-hooks": "^4.6.0",
        "eslint-plugin-react-refresh": "^0.4.5",
        "husky": "^9.1.7",
        "jest": "^29.7.0",
        "lint-staged": "^15.2.10",
        "prettier": "^3.3.2",
        "react": "^18.3.1",
        "react-dom": "^18.3.1",
        "tailwindcss": "^3.4.7",
        "ts-jest": "^29.2.5",
        "typescript": "^5.5.2",
        "vite": "^5.3.1",
        "vite-plugin-electron": "^0.28.6",
        "vite-plugin-electron-renderer": "^0.14.5"
    },
    "husky": {
        "hooks": {
            "pre-commit": "lint-staged"
        }
    },
    "lint-staged": {
        "*.{js,jsx,ts,tsx}": [
            "npm run format"
        ]
    }
}
